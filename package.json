{
  "name": "bakedbliss-backend",
  "version": "1.0.0",
  "description": "A Baked bliss application",
  "main": "app.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "node --watch app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.js\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.js\" --ignore-path .gitignore",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [
    "bakery",
    "ecommerce",
    "nodejs",
    "express"
  ],
  "author": "akash",
  "license": "ISC",
  "dependencies": {
    "console-stamp": "^3.1.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "eb-butler-utils": "^1.0.11",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "nodemailer": "^7.0.12",
    "sanitize-html": "^2.17.0",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^2.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "supertest": "^6.3.4"
  }
}